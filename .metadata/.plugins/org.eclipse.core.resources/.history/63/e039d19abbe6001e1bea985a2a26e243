		fosko.setNbPattes(3);
		menagerie.ajouter(fosko);
		Serpent sneko = new Serpent("Sepent Ã  sonette", 5, 300, true);
		menagerie.ajouter(sneko);
		Canard donald = new Canard("Mullard", 4);
		menagerie.ajouter(donald);
		
		//System.out.println(menagerie);
