package sae.graphe;

import sae.dungeon.Coord;


public class Node {
	private String name;
	private Node predecesseur;
	private Coord coords;
	
	public Node(String name, Coord coords) {
		super();
		this.coords = coords;
		this.name = name;
	}
	
	public void addPredecesseur(Node node) {
		predecesseur = node;
	}
	
	public Node getPredecesseur() {
		return predecesseur;
	}
	
	
	public Coord getCoords() {
		return coords;
	}
	

}
