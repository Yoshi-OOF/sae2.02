package sae.graphe;

import java.util.HashMap;
import java.util.Map;

import sae.dungeon.Coord;
import sae.dungeon.Direction;
import sae.dungeon.Room;

public class Node {
	private String name;
	private Node predecesseur;
	private Coord coords;
	
	public Node(String name, Coord coords) {
		super();
		this.coords = coords;
		this.name = name;
	}
	
	public void addPredecesseur(Node node) {
		predecesseur = node;
	}
	
	public Node getPredecesseur() {
		return predecesseur;
	}
	

}
